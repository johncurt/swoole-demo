{
    "name": "slim-with-swoole-and-websockets",
    "description": "A Slim Framework skeleton application for rapid development",
    "license": "MIT",
    "authors": [
        {
            "name": "John Fansler",
            "email": "john.fansler@gmail.com",
            "homepage": "http://www.johnfansler.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "pachico/slim-swoole": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "WebSocket\\": "src/WebSocket/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php ./public/index.php",
        "test": "phpunit"
    }

}
